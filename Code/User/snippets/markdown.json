{
	"Greyed out":{
		"prefix": "grey",
		"body": [
			"<span style=\"color: lightgrey;\">$TM_SELECTED_TEXT</span>",
			
		],
		"description": "HTML to grey out some text"
	},

	"Snippet":{
		"prefix": "snippet",
		"body": [
			"--8<-- \"${1:/path/to/snippet}\""
		],
		"description": "Mkdocs Material snippet"
	},

	"Cloud providers":{
		"prefix": "cloud",
		"body": [
			"=== \":material-google:\"",
			"",
			"    ```sh",
			"    $0",
			"    ```",
			"",
			"=== \":material-microsoft-azure:\"",
			"",
			"    ```sh",
			"    ",
			"    ```",
			"",
		],
		"description": "Mkdocs Material tabs for 3 cloud providers (2 for now), using their emoji"
	},

	"Mkdocs Material tab": {
		"prefix": "tab",
		"body": [
			"=== \"${1:Tab}\"",
			"",
			"\t${TM_SELECTED_TEXT}",
			""
		]
	}
}
