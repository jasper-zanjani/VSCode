{
	"Greyed out":{
		"prefix": "grey",
		"body": [
			"<span style=\"color: lightgrey;\">$TM_SELECTED_TEXT</span>",
			
		],
		"description": "HTML to grey out some text"
	},

	"Snippet":{
		"prefix": "snippet",
		"body": [
			"--8<-- \"${1:/path/to/snippet}\""
		],
		"description": "Mkdocs Material snippet"
	},

	"Cloud providers":{
		"prefix": "cloud",
		"body": [
			"=== \":material-google:\"",
			"",
			"    ```sh",
			"    $0",
			"    ```",
			"",
			"=== \":material-microsoft-azure:\"",
			"",
			"    ```sh",
			"    ",
			"    ```",
			"",
		],
		"description": "Mkdocs Material tabs for 3 cloud providers (2 for now), using their emoji"
	},

	"Mkdocs Material tab": {
		"prefix": "tab",
		"body": [
			"=== \"${1:Tab}\"",
			"",
			"\t${TM_SELECTED_TEXT}",
			""
		]
	},
	"CRUD tabs": {
		"prefix": "crud",
		"body": [
			"=== \":material-plus: Create\"",
			"",
			"    ```sh",
			"    ${TM_SELECTED_TEXT:command} ${1:add}",
			"    ```",
			"",
			"=== \":material-eye: Read\"",
			"",
			"    ```sh",
			"    ${TM_SELECTED_TEXT:command} ${2:get}",
			"    ```",
			"",
			"=== \":material-format-list-text: List\"",
			"",
			"    ```sh",
			"    ${TM_SELECTED_TEXT:command} ${3:list}",
			"    ```",
			"",
			"=== \":material-pencil: Update\"",
			"",
			"    ```sh",
			"    ${TM_SELECTED_TEXT:command} ${4:set}",
			"    ```",
			"",
			"=== \":material-trash-can: Delete\"",
			"",
			"    ```sh",
			"    ${TM_SELECTED_TEXT:command} ${5:remove}",
			"    ```",

		]
	},
}
